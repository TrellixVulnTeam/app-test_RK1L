XJ,YAAA,CAUM,KAVN,EAAA,UAAA,EAUM,CATJ,YAAA,CAAgD,KAAhD,EAAgD;AAA3C,IAAA,SAAoC,EAA5B,QAAA,CAAA,aAAA,CAAc,IAAA,CAAA,OAAA,CAAQ,IAAtB;AAAmC,GAAhD,E,IAAA,E;;AAAA,I,aAAA,CASI,EARuB,IAAA,CAAA,OAAA,CAAQ,OAAR,IAAmB,IAAA,CAAA,OAAA,CAAQ,OAAR,CAAgB,M,kBAA9D,YAAA,CAOK,IAPL,EAAA,UAAA,EAOK,E,kBANH,YAAA,CAKK,SALL,EAKK,IALL,EAKK,WAAA,CALsB,IAAA,CAAA,OAAA,CAAQ,OAK9B,EALqC,CAA9B,GAA8B,EAAzB,KAAyB,KAApB;yBAAtB,YAAA,CAKK,IALL,EAKK;AALwC,MAAA,GAAG,EAAE,GAAG,GAAG;AAKnD,KALL,E,CACE,YAAA,CAGS,gBAHT,EAGS;AAFN,MAAA,IAAI,EAAE,GAEA;AADP,MAAA,SAAS,EAAC;AACH,KAHT,E,IAAA,E;;AAAA,M,QAAA,C,CADF,C;GAKK,CALL,E;;AAAA,GAMG,EAPL,C,qCAQI,CAVN,CAWI,CAZN,CAiBI,EAJJ,UAII,EAHJ,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,YAAA,CAAkE,wBAAlE,EAAkE;AAAlD,IAAA,OAAO,EAAE,IAAA,CAAA,OAAyC;AAA/B,IAAA,OAAO,EAAE;AAAsB,GAAlE,E,IAAA,E;;AAAA,I,WAAA,CACI,CAFN,CAGI,CAlBN,CAmBI,CA1BN,C","sourcesContent":["<template>\n  <div class=\"article-page\">\n    <div class=\"banner\">\n      <div class=\"container\">\n        <h1>{{ article.title }}</h1>\n        <VtArticleMeta :article=\"article\" :actions=\"true\"></VtArticleMeta>\n      </div>\n    </div>\n    <div class=\"container page\">\n      <div class=\"row article-content\">\n        <div class=\"col-xs-12\">\n          <div v-html=\"parseMarkdown(article.body)\"></div>\n          <ul class=\"tag-list\" v-if=\"article.tagList && article.tagList.length\">\n            <li v-for=\"(tag, index) of article.tagList\" :key=\"tag + index\">\n              <VtTag\n                :name=\"tag\"\n                className=\"tag-default tag-pill tag-outline\"\n              ></VtTag>\n            </li>\n          </ul>\n        </div>\n      </div>\n      <hr />\n      <div class=\"article-actions\">\n        <VtArticleMeta :article=\"article\" :actions=\"true\"></VtArticleMeta>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport marked from \"marked\";\nimport store from \"@/store\";\nimport VtArticleMeta from \"@/components/ArticleMeta\";\nimport VtTag from \"@/components/VTag\";\nimport { FETCH_ARTICLE, FETCH_COMMENTS } from \"@/store/actions.type\";\n\nexport default {\n  name: \"Vt-article\",\n  props: {\n    slug: {\n      type: String,\n      required: true\n    }\n  },\n  components: {\n    VtArticleMeta,\n    VtTag\n  },\n  beforeRouteEnter(to, from, next) {\n    Promise.all([\n      store.dispatch(FETCH_ARTICLE, to.params.slug),\n      store.dispatch(FETCH_COMMENTS, to.params.slug)\n    ]).then(() => {\n      next();\n    });\n  },\n  computed: {\n    ...mapGetters([\"article\", \"currentUser\", \"comments\", \"isAuthenticated\"])\n  },\n  methods: {\n    parseMarkdown(content) {\n      return marked(content);\n    }\n  }\n};\n</script>\n"],"sourceRoot":""}]}